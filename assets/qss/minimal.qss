
#TitleBar {
    background-color: #ffffff;
    border-bottom: 1px solid #cccccc;
}
#TitleBar QLabel {
    font-size: 14px;
    font-weight: normal;
    color: #333333;
}
#TitleBar QPushButton {
    background: transparent;
    color: #333333;
    border: none;
    font-size: 14px;
    padding: 0 8px;
}
#TitleBar QPushButton:hover {
    background-color: #eeeeee;
}
